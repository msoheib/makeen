{
  "name": "property-management-system",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env EXPO_NO_TELEMETRY=1 EXPO_DEBUG=1 expo start --web",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "detox:build:android:release": "detox build --configuration android.emu.release",
    "detox:test:android:release": "detox test --configuration android.emu.release",
    "e2e:test": "npm run detox:build:android && npm run detox:test:android",
    "e2e:test:rtl": "detox test --configuration android.emu.debug e2e/rtl.test.js",
    "e2e:test:forms": "detox test --configuration android.emu.debug e2e/forms/",
    "e2e:test:auth": "detox test --configuration android.emu.debug e2e/forms/auth.test.js",
    "e2e:test:property": "detox test --configuration android.emu.debug e2e/forms/property.test.js",
    "e2e:test:tenant": "detox test --configuration android.emu.debug e2e/forms/tenant.test.js",
    "e2e:test:maintenance": "detox test --configuration android.emu.debug e2e/forms/maintenance.test.js",
    "e2e:test:financial": "detox test --configuration android.emu.debug e2e/forms/financial.test.js",
    "e2e:test:all-forms": "detox test --configuration android.emu.debug e2e/forms/all-forms.test.js",
    "e2e:test:full": "npm run detox:build:android && npm run e2e:test:forms",
    "clear-auth": "node scripts/clear-auth.js",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-picker/picker": "^2.11.0",
