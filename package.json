    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.3.4",
    "@supabase/supabase-js": "^2.39.7",
    "base64-arraybuffer": "^1.0.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "expo": "53.0.11",
