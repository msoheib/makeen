{
  "name": "property-management-system",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:web": "vite build",
    "typecheck": "tsc",
    "preview": "vite preview",
    "setup:test": "node scripts/setup-test-env.js",
    "dev:native": "cross-env EXPO_NO_TELEMETRY=1 EXPO_DEBUG=1 expo start --web",
    "build:native": "expo export --platform web",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "detox:build:android:release": "detox build --configuration android.emu.release",
    "detox:test:android:release": "detox test --configuration android.emu.release",
    "e2e:test": "npm run detox:build:android && npm run detox:test:android",
    "e2e:test:rtl": "detox test --configuration android.emu.debug e2e/rtl.test.js",
    "e2e:test:forms": "detox test --configuration android.emu.debug e2e/forms/",
    "e2e:test:auth": "detox test --configuration android.emu.debug e2e/forms/auth.test.js",
    "e2e:test:property": "detox test --configuration android.emu.debug e2e/forms/property.test.js",
    "e2e:test:tenant": "detox test --configuration android.emu.debug e2e/forms/tenant.test.js",
    "e2e:test:maintenance": "detox test --configuration android.emu.debug e2e/forms/maintenance.test.js",
    "e2e:test:financial": "detox test --configuration android.emu.debug e2e/forms/financial.test.js",
    "e2e:test:all-forms": "detox test --configuration android.emu.debug e2e/forms/all-forms.test.js",
    "e2e:test:full": "npm run detox:build:android && npm run e2e:test:forms",
    "clear-auth": "node scripts/clear-auth.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint:i18n": "node scripts/lint-i18n.js",
    "test:playwright": "npx playwright test",
    "test:playwright:ui": "npx playwright test --ui",
    "test:qa": "node playwright/qa-runner.js",
    "test:qa:theme": "npx playwright test qa-theme-language.spec.ts",
    "test:qa:theme:ui": "npx playwright test qa-theme-language.spec.ts --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/cairo": "^5.1.0",
    "@fontsource/inter": "^5.1.0",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/x-date-pickers": "^7.22.2",
    "@supabase/supabase-js": "^2.39.7",
    "chart.js": "^4.4.7",
    "chartjs-plugin-datalabels": "^2.2.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "html2canvas": "^1.4.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.2",
    "react-router-dom": "^7.1.1",
    "react-window": "^1.8.10",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-i18next": "^7.8.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "jest": "~29.7.0",
    "playwright": "^1.55.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^5.4.11"
  }
}
